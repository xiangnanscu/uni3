{
  "name": "uni-preset-vue",
  "version": "1.119.0",
  "license": "ISC",
  "scripts": {
    "env": "dotenv -e .env -e .env.local --",
    "cli": "C:/Users/Admin/Documents/HBuilderX/cli.exe",
    "preuni": "node bin/views-checker.js",
    "uni": "yarn env uni",
    "uni:build": "vue-tsc --noEmit && npm --no-git-tag-version version minor && yarn uni build",
    "dev": "yarn uni",
    "devw": "bash bin/wx-dev.sh",
    "build": "yarn uni:build",
    "buildw": "yarn uni:build -p mp-weixin",
    "rcw": "yarn buildw && node bin/wx-release.js",
    "git": "python3 bin/git.py",
    "b": "yarn p -bb -gg",
    "p": "yarn git -pp -gg",
    "g": "yarn git -gg",
    "gg": "yarn g x",
    "sql": "if grep -q 'DEBUG_SQL=off' .env ; then yarn sqlon; else yarn sqloff; fi",
    "sqlon": "sed -i 's/DEBUG_SQL=off/DEBUG_SQL=on/g' .env && yarn nginx:reload",
    "sqloff": "sed -i 's/DEBUG_SQL=on/DEBUG_SQL=off/g' .env && yarn nginx:reload",
    "r": "yarn release",
    "rm": "yarn release:migrate",
    "rc": "yarn build && yarn release",
    "rcm": "yarn build && yarn release:migrate",
    "commit": "git add . && git commit -am ",
    "cm": "yarn commit",
    "push": "yarn commit",
    "postpush": "while true; do git push origin master && { echo 'Git push succeeded'; break; } || echo 'Git push failed, retrying in 1 seconds'; sleep 1; done",
    "pull": "while true; do git pull origin master && { echo 'Git pull succeeded'; break; } || echo 'Git pull failed, retrying in 1 seconds'; sleep 1; done",
    "release": "yarn commit; git push p master && git push origin master",
    "release:migrate": "yarn commit; git push p master --push-option=migrate:commit && git push origin master",
    "boot": "node bin/boot.js",
    "boot:prod": "NODE_ENV=production yarn boot",
    "boot:log": "bin/nginx-logrotate.sh",
    "pg": "service postgresql start ; killall -9 nginx;killall -9 openresty; yarn nginx ; yarn dev",
    "resty": "resty -I lualib -I resty_modules/lualib -I resty_modules/site/lualib --main-conf 'env NODE_ENV;' --http-conf 'lua_ssl_trusted_certificate /etc/ssl/certs/ca-certificates.crt;'",
    "psql": "yarn env psql",
    "migrate": "yarn resty bin/migrate.lua",
    "migrate:commit": "yarn migrate -commit",
    "mc": "yarn migrate -commit",
    "ms": "yarn migrate -snap",
    "mcl": "yarn migrate -commit-latest",
    "mr": "yarn migrate -snap -diff -append",
    "mrc": "yarn mr -commit",
    "mrs": "yarn migrate -commit && yarn ms",
    "mrr": "yarn migrate -clear && yarn mrc",
    "make-conf": "yarn env ./bin/make-conf.sh",
    "views": "node --trace-uncaught bin/views.js",
    "nginx": "yarn make-conf && yarn nginx:stop; yarn nginx:start",
    "nginx:start": "openresty -p $PWD/ -c conf/nginx.conf",
    "nginx:stop": "yarn nginx:start -s stop",
    "nginx:reload": "yarn make-conf && yarn nginx:start -s reload",
    "rr": "npm run rename && npm run replace",
    "find": "find . -type d \\( -name .git -o -name node_modules -o -name dist \\) -prune -o -type f -name \"*.${ext:-*}\"",
    "f": "yarn find -print0| xargs -0 grep -n ",
    "replace": "yarn find -exec  sed -i s/${old}/${new}/g {} \\;",
    "rename": "rename s/xodel/model/g src/*",
    "vpn": "sshuttle --dns -r root@rsks.ren 0.0.0.0/0 -x rsks.ren",
    "npm:reset": "rm -rf node_modules && npm ci",
    "re": "yarn nginx:reload",
    "prisma": "prisma-include prisma/schema.include.prisma > prisma/schema.prisma && yarn env prisma",
    "busted": "yarn resty -I spec bin/busted.lua -o utfTerminal",
    "set-url": "git remote set-url origin git@gitee.com:xiangnanscu/jaqn.git",
    "add-p": "git remote add p root@gqtjaxw.cn:~/jaqn/.git",
    "add-g": "git remote add g git@github.com:xiangnanscu/jaqn.git",
    "preview": "vite preview --port 4173",
    "sync": "ssh root@gqtjaxw.cn \"PGPASSWORD=postgres pg_dump -c -Upostgres -hlocalhost jaqn\" > /tmp/jaqn.sql && PGPASSWORD=postgres psql -Upostgres -hlocalhost -d jaqn -f /tmp/jaqn.sql|grep ERROR",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "yarn busted",
    "jest": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "diff": "diff --exclude=node_modules --exclude=.git --exclude=logs  --exclude=dist -bur ~/xodel ~/jahy",
    "stat": "bin/goaccess-stat.sh",
    "update": "bin/update-mini.sh",
    "postinstall": "patch-package",
    "pu": "npx patch-package @dcloudio/uni-ui",
    "pua": "npm remove @dcloudio/uni-ui && npm install @dcloudio/uni-ui",
    "type-check": "vue-tsc --noEmit"
  },
